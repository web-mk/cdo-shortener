{
  "name": "CDO Shortener",
  "description": "Grab the shortest possible Chabad.org URL",
  "version": "0.0.2",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Grab the shortest possible Chabad.org URL",
    "default_icon": {
      "16": "images/cdo-icon-16.png",
      "32": "images/cdo-icon-32.png",
      "48": "images/cdo-icon-48.png",
      "128": "images/cdo-icon-128.png"
    }
  },
  "commands": {
    "shorten": {
      "suggested_key": {
        "default": "Ctrl+Shift+O"
      },
      "description": "Grab the short URL"
    }
  },
  "icons": {
    "16": "images/cdo-icon-16.png",
    "32": "images/cdo-icon-32.png",
    "48": "images/cdo-icon-48.png",
    "128": "images/cdo-icon-128.png"
  },
  "manifest_version": 2
}
